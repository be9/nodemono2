{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build:self": {
      "dependsOn": [
        "^build:self"
      ],
      "outputs": [
        "build/**"
      ]
    },
    "clean": {
      "cache": false
    }
  },
  "globalDependencies": [
    "tsconfig.json"
  ],
  "remoteCache": {
    "signature": true
  }
}
